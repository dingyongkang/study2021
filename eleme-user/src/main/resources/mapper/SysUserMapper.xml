<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.admindao.SysUserDao">
<resultMap id="BaseResult" type="com.qf.pojo.Tb_Sys_User">
    <id column="userid" property="userid"></id>
    <result column="login_name" property="login_name"></result>
    <result column="password" property="password"></result>
    <result column="state" property="state"></result>
    <result column="create_time" property="create_time"></result>
    <result column="realname" property="realname"></result>
</resultMap>
    <select id="findByUserName" parameterType="String" resultMap="BaseResult">
        select * from tb_sys_user where login_name=#{username}
    </select>
</mapper>